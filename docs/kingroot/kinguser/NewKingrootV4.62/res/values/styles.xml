    </style>
    <style name="list_item_secondary_title_style_km" parent="@style/text_secondary_style">
        <item name="android:textSize">@dimen/list_item_text_secondary</item>
        <item name="android:textColor">@color/list_item_text_secondary_color_km</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/list_item_text_secondary_marginTop</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="list_item_extends_title_style" parent="@style/text_secondary_style">
        <item name="android:textSize">@dimen/text_text_thirdly_size</item>
        <item name="android:textColor">@color/list_item_text_extends_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="PopupAnimation" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/window_show_anim</item>
        <item name="android:windowExitAnimation">@anim/window_hide_anim</item>
    </style>
</resources>
