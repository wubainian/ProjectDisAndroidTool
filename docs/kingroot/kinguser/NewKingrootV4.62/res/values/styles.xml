<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme" parent="@android:style/Theme" />
    <style name="Theme.NoBackground" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="Theme.NoTitle" parent="@android:style/Theme">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="Theme.MainActivity" parent="@android:style/Animation.Translucent">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@style/activityAnimation</item>
    </style>
    <style name="Theme.NoTitleAndTransAnim" parent="@android:style/Animation.Translucent">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@style/activityAnimation</item>
    </style>
    <style name="activityAnimation" parent="@android:style/Animation">
        <item name="android:activityOpenEnterAnimation">@anim/logcat_enter_in</item>
        <item name="android:activityOpenExitAnimation">@anim/logcat_not_change</item>
        <item name="android:activityCloseEnterAnimation">@anim/logcat_not_change</item>
        <item name="android:activityCloseExitAnimation">@anim/logcat_exit_out</item>
    </style>
    <style name="Theme.RequestActivityTheme" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="Theme.Dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="Theme.RememberCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/selector_checkbox</item>
    </style>
    <style name="slider_header_title_style">
        <item name="android:textSize">@dimen/text_primary_size</item>
        <item name="android:textColor">@color/text_primary_color</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="slider_header_title_line_style">
        <item name="android:background">@color/blue_1</item>
        <item name="android:layout_height">2.0dip</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="commonCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_selector_2</item>
    </style>
    <style name="main_title2_text_style">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="main_title2_view_style">
        <item name="android:background">@color/global_primary_blue_color</item>
        <item name="android:layout_height">2.0dip</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
    <style name="text_primary_uninstall_style">
        <item name="android:textSize">@dimen/text_title_size</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@android:string/unknownName</item>
    </style>
    <style name="title_bar_style">
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@color/global_primary_blue_color</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/title_bar_height</item>
    </style>
    <style name="list_header_bar_style">
        <item name="android:background">@color/sub_title_bg</item>
        <item name="android:paddingLeft">@dimen/content_marginLeft</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/list_header_bar_height</item>
    </style>
    <style name="list_item_style">
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/selector_list_item_bg</item>
        <item name="android:paddingLeft">@dimen/list_item_paddingLeft</item>
        <item name="android:paddingTop">@dimen/list_item_paddingTop</item>
        <item name="android:paddingRight">@dimen/list_item_paddingLeft</item>
        <item name="android:paddingBottom">@dimen/list_item_paddingBottom</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/list_item_minHeight</item>
    </style>
    <style name="list_item_style_km">
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/list_item_bg_selector</item>
        <item name="android:paddingLeft">@dimen/list_item_paddingLeft</item>
        <item name="android:paddingTop">@dimen/list_item_paddingTop</item>
        <item name="android:paddingRight">@dimen/list_item_paddingLeft</item>
        <item name="android:paddingBottom">@dimen/list_item_paddingBottom</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/list_item_minHeight</item>
    </style>
    <style name="list_item_icon_container_style">
        <item name="android:layout_width">@dimen/list_item_icon_width</item>
        <item name="android:layout_height">@dimen/list_item_icon_height</item>
        <item name="android:layout_marginRight">@dimen/list_item_icon_marginRight</item>
    </style>
    <style name="list_item_checkbox_style" parent="@style/custom_checkbox">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="list_item_checkbox_style_km">
        <item name="android:background">@drawable/checkbox_selector</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/list_item_checkbox_marginLeft</item>
        <item name="android:button">@null</item>
    </style>
    <style name="list_item_tag_style" parent="@style/list_item_secondary_title_style">
        <item name="android:textColor">@color/allow_label</item>
    </style>
    <style name="common_button_style">
        <item name="android:textSize">@dimen/text_title_size</item>
        <item name="android:textColor">@color/text_title_color</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/common_btn_selector</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/button_height</item>
        <item name="android:layout_marginLeft">@dimen/button_margin_horinzontal</item>
        <item name="android:layout_marginTop">@dimen/button_margin_vertical</item>
        <item name="android:layout_marginRight">@dimen/button_margin_horinzontal</item>
        <item name="android:layout_marginBottom">@dimen/button_margin_vertical</item>
    </style>
    <style name="common_dialog_button_style">
        <item name="android:textSize">@dimen/dialog_btn_size</item>
        <item name="android:textColor">@color/dialog_bottom_btn_text</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">@dimen/dialg_buttom_height</item>
    </style>
    <style name="common_dialog_button_style_km">
        <item name="android:textSize">@dimen/dialog_btn_size_km</item>
        <item name="android:textColor">@color/dialog_bottom_btn_text_km</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">@dimen/dialg_buttom_height_km</item>
    </style>
    <style name="common_dialog_button_style.left" parent="@style/common_dialog_button_style">
        <item name="android:background">@drawable/common_dialog_button_left_selector</item>
    </style>
    <style name="common_dialog_button_cancel_style" parent="@style/common_dialog_button_style.left">
        <item name="android:textColor">@color/dialog_bottom_cancel_color_km</item>
    </style>
    <style name="common_dialog_button_style.right" parent="@style/common_dialog_button_style">
        <item name="android:background">@drawable/common_dialog_button_right_selector</item>
    </style>
    <style name="common_dialog_button_style.middle" parent="@style/common_dialog_button_style">
        <item name="android:background">@drawable/common_dialog_button_mid_selector</item>
    </style>
    <style name="common_dialog_title_style">
        <item name="android:paddingLeft">@dimen/dialog_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/dialog_padding_horizontal</item>
        <item name="android:layout_height">@dimen/dialog_title_height</item>
    </style>
    <style name="common_dialog_title_style_km">
        <item name="android:paddingLeft">@dimen/dialog_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/dialog_padding_horizontal</item>
        <item name="android:layout_height">@dimen/dialog_title_height_km</item>
    </style>
    <style name="Theme.NoTitleAndTransAnimAndTransparent" parent="@android:style/Theme">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/activityAnimation</item>
    </style>
    <style name="NoDisplay">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowNoDisplay">true</item>
        <item name="android:windowDisablePreview">true</item>
    </style>
    <style name="custom_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/selector_switcher</item>
    </style>
    <style name="list_item_two_lines_style">
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/selector_list_item_bg</item>
        <item name="android:paddingLeft">@dimen/list_item_padding_left</item>
        <item name="android:paddingTop">@dimen/list_item_padding_top</item>
        <item name="android:paddingRight">@dimen/list_item_padding_left</item>
        <item name="android:paddingBottom">@dimen/list_item_padding_bottom</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/list_item_tow_lines_min_height</item>
    </style>
    <style name="list_item_single_line_style">
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/selector_list_item_bg</item>
        <item name="android:paddingLeft">@dimen/list_item_padding_left</item>
        <item name="android:paddingTop">@dimen/list_item_padding_top</item>
        <item name="android:paddingRight">@dimen/list_item_padding_left</item>
        <item name="android:paddingBottom">@dimen/list_item_padding_bottom</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/list_item_single_line_min_height</item>
    </style>
    <style name="list_item_icon_style">
        <item name="android:background">@null</item>
        <item name="android:layout_width">@dimen/list_item_icon_size</item>
        <item name="android:layout_height">@dimen/list_item_icon_size</item>
        <item name="android:src">@drawable/icon</item>
    </style>
    <style name="list_item_icon_style_km">
        <item name="android:layout_width">@dimen/list_item_icon_width</item>
        <item name="android:layout_height">@dimen/list_item_icon_height</item>
        <item name="android:layout_marginRight">@dimen/list_item_icon_marginRight</item>
        <item name="android:src">@drawable/default_icon</item>
    </style>
    <style name="text_primary_style">
        <item name="android:textSize">@dimen/text_primary_size</item>
        <item name="android:textColor">@color/text_primary_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@android:string/unknownName</item>
    </style>
    <style name="text_primary_style_km">
        <item name="android:textSize">@dimen/text_primary_size</item>
        <item name="android:textColor">@color/text_primary_color_km</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@android:string/unknownName</item>
    </style>
    <style name="text_secondary_style">
        <item name="android:textSize">@dimen/text_secondary_size</item>
        <item name="android:textColor">@color/text_secondary_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@android:string/unknownName</item>
    </style>
    <style name="text_secondary_style_km">
        <item name="android:textSize">@dimen/text_secondary_size</item>
        <item name="android:textColor">@color/text_secondary_color_km</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@android:string/unknownName</item>
    </style>
    <style name="list_item_primary_title_style" parent="@style/text_primary_style">
        <item name="android:textSize">@dimen/list_item_text_primary</item>
        <item name="android:textColor">@color/list_item_text_primary_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@android:string/unknownName</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="list_item_primary_title_style_km" parent="@style/text_primary_style">
        <item name="android:textSize">@dimen/list_item_text_primary</item>
        <item name="android:textColor">@color/list_item_text_primary_color_km</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@android:string/unknownName</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="list_item_secondary_title_style" parent="@style/text_secondary_style">
        <item name="android:textSize">@dimen/list_item_text_secondary</item>
        <item name="android:textColor">@color/list_item_text_secondary_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/list_item_text_secondary_marginTop_km</item>
